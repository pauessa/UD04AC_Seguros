<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping">

<hibernate-mapping>
  <class name="model.Direccion" table="direccion">
      <id name="id" column="Id" type="integer"/>
        <property name="calle" />
        <property name="numero" />
        <property name="poblacion" />
        <property name="provincia" />
  </class>
</hibernate-mapping>